   "source": [
    "import numpy as np\n",
    "\n",
    "def f(x):\n",
    "    return np.tan(x)\n",
    "def derf(x):\n",
    "    return 1+np.tan(x)**2\n",
    "\n",
    "x = np.pi/2 + 0.000005\n",
    "delta_x = 0.000005\n",
    "\n",
    "k1 = (np.abs(f(x + delta_x) - f(x)) / np.abs(f(x))) / (delta_x/x)\n",
    "k2 = np.abs(derf(x) * x / f(x))\n",
    "\n",
    "print(k1)\n",
    "print(k2)\n"
   ]
